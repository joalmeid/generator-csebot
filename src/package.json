      "inttest": "istanbul cover --report cobertura --report lcov node_modules/mocha/bin/_mocha -- -R xunit-file integration/test",
      "version": "",
      "postversion": "git push && git push --tags"
   },
   "files": [
      "generators/app",
      "generators/bbv3-csharp",
